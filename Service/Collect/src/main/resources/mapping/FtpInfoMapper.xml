<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hzgc.service.collect.dao.FtpInfoMapper" >
  <resultMap id="BaseResultMap" type="com.hzgc.service.collect.model.FtpInfo" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="count" property="count" jdbcType="INTEGER" />
    <result column="ip" property="ip" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, count, ip
  </sql>
  <select id="selectByFtpAddress" resultMap="BaseResultMap" parameterType="java.lang.String">
    select
    <include refid="Base_Column_List"/>
    from t_ftp
    where ip = #{ip,jdbcType=VARCHAR}
  </select>
  <select id="selectByCountAsc" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List"/>
    from t_ftp order by count ASC
  </select>
  <insert id="insertSelective" parameterType="com.hzgc.service.collect.model.FtpInfo" >
    insert into t_ftp
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="count != null" >
        count,
      </if>
      <if test="ip != null" >
        ip,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="count != null" >
        #{count,jdbcType=INTEGER},
      </if>
      <if test="ip != null" >
        #{ip,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <update id="updateByIpSelective" parameterType="com.hzgc.service.collect.model.FtpInfo" >
    update t_ftp
    <set >
      <if test="count != null" >
        count = #{count,jdbcType=INTEGER},
      </if>
    </set>
    where ip = #{ip,jdbcType=VARCHAR}
  </update>
</mapper>